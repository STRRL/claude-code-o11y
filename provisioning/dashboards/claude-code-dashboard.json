{
  "id": null,
  "title": "Claude Code Analytics",
  "tags": ["claude-code", "analytics"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "panels": [
    {
      "id": 1,
      "title": "üìà Lines of Code Changed (1h)",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(increase(claude_code_lines_of_code_count{type=\"added\"}[1h]))",
          "refId": "A",
          "legendFormat": "Added",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "sum(increase(claude_code_lines_of_code_count{type=\"removed\"}[1h]))",
          "refId": "B",
          "legendFormat": "Removed",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "auto",
        "colorMode": "value"
      }
    },
    {
      "id": 2,
      "title": "üìà Code Generation Productivity",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by () (rate(claude_code_lines_of_code_count{type=\"added\"}[5m])) * 60",
          "refId": "A",
          "legendFormat": "Lines Added per Minute",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "sum by () (rate(claude_code_lines_of_code_count{type=\"removed\"}[5m])) * 60",
          "refId": "B", 
          "legendFormat": "Lines Removed per Minute",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 3,
      "title": "‚ö° Performance Metrics (1h)",
      "type": "table",
      "targets": [
        {
          "expr": "sum(increase(claude_code_lines_of_code_count{type=\"added\"}[1h])) / sum(increase(claude_code_cost_usage[1h]))",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 6},
      "transformations": [
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "Value": "Lines per Dollar"
            }
          }
        }
      ]
    },
    {
      "id": 4,
      "title": "üéØ AI Suggestion Accept Rate (All Time)",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(claude_code:tool_decisions_total{decision=\"accept\"}) / sum(claude_code:tool_decisions_total) * 100",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 6},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 60},
              {"color": "green", "value": 80}
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "üî¢ Token Usage Distribution (All Time)",
      "type": "piechart",
      "targets": [
        {
          "expr": "claude_code:token_total_by_type",
          "refId": "A",
          "legendFormat": "{{type}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 12},
      "options": {
        "pieType": "donut",
        "displayLabels": ["name", "value"]
      }
    },
    {
      "id": 6,
      "title": "‚ö° Cache Hit Rate (1h)",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(increase(claude_code_token_usage{type=\"cacheRead\"}[1h])) / sum(increase(claude_code_token_usage{type=~\"input|cacheRead\"}[1h])) * 100",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 12},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "green", "value": 90}
            ]
          }
        }
      }
    },
    {
      "id": 7,
      "title": "üí∞ Total Cost (All Time)",
      "type": "stat",
      "targets": [
        {
          "expr": "claude_code:cost_total",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 18},
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 4,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "orange", "value": 5},
              {"color": "red", "value": 10}
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "auto",
        "colorMode": "background"
      }
    },
    {
      "id": 8,
      "title": "üí∞ Real-time Cost Rate by Model ($/min)",
      "type": "table",
      "targets": [
        {
          "expr": "sum by (model) (rate(claude_code_cost_usage[5m]) * 60)",
          "refId": "A",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 18},
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 4
        }
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "model": "Model",
              "Value": "$/min"
            }
          }
        }
      ]
    },
    {
      "id": 9,
      "title": "üí∏ Cost Trend by Model",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by (model) (rate(claude_code_cost_usage[5m])) * 60",
          "refId": "A",
          "legendFormat": "{{model}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 24},
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        }
      }
    },
    {
      "id": 10,
      "title": "üõ†Ô∏è Tool Acceptance Rate by Type",
      "type": "barchart",
      "targets": [
        {
          "expr": "sum by (tool_name) (increase(claude_code_code_edit_tool_decision{decision=\"accept\"}[1h])) / sum by (tool_name) (increase(claude_code_code_edit_tool_decision[1h])) * 100",
          "refId": "A",
          "legendFormat": "{{tool_name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 24},
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "id": 11,
      "title": "üìä Programming Language Acceptance",
      "type": "barchart", 
      "targets": [
        {
          "expr": "sum by (language) (increase(claude_code_code_edit_tool_decision{decision=\"accept\"}[1h])) / sum by (language) (increase(claude_code_code_edit_tool_decision[1h])) * 100",
          "refId": "A",
          "legendFormat": "{{language}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 30},
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "id": 12,
      "title": "üéØ AI Effectiveness Score",
      "type": "stat",
      "targets": [
        {
          "expr": "(sum(increase(claude_code_code_edit_tool_decision{decision=\"accept\"}[1h])) / sum(increase(claude_code_code_edit_tool_decision[1h]))) * (sum(increase(claude_code_token_usage{type=\"cacheRead\"}[1h])) / sum(increase(claude_code_token_usage{type=~\"input|cacheRead\"}[1h]))) * 100",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 30},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 75}
            ]
          }
        }
      },
      "description": "Combined metric: Accept Rate √ó Cache Hit Rate"
    },
    {
      "id": 13,
      "title": "üí∞ Total Cost by Model (All Time)",
      "type": "piechart",
      "targets": [
        {
          "expr": "claude_code:cost_total_by_model",
          "refId": "A",
          "legendFormat": "{{model}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 36},
      "options": {
        "pieType": "donut",
        "displayLabels": ["name", "value"]
      },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 4
        }
      }
    },
    {
      "id": 14,
      "title": "üìà Total Lines of Code (All Time)",
      "type": "stat",
      "targets": [
        {
          "expr": "claude_code:lines_total_by_type{type=\"added\"}",
          "refId": "A",
          "legendFormat": "Added",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "claude_code:lines_total_by_type{type=\"removed\"}",
          "refId": "B",
          "legendFormat": "Removed",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 36},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "auto",
        "colorMode": "value"
      }
    },
    {
      "id": 15,
      "title": "üë• Top Users by Activity",
      "type": "table",
      "targets": [
        {
          "expr": "topk(10, sum by (user_email) (increase(claude_code_token_usage[1h])))",
          "refId": "A",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 42},
      "transformations": [
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "user_email": "User",
              "Value": "Tokens Used (1h)"
            }
          }
        }
      ]
    },
    {
      "id": 16,
      "title": "üèÜ Tool Decision Summary (All Time)",
      "type": "table",
      "targets": [
        {
          "expr": "claude_code:tool_decisions_total",
          "refId": "A",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 42},
      "transformations": [
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "decision": "Decision",
              "tool_name": "Tool",
              "language": "Language",
              "Value": "Count"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 17,
      "title": "üìà Total Tokens by Type (All Time)",
      "type": "stat",
      "targets": [
        {
          "expr": "claude_code:token_total_by_type{type=\"input\"}",
          "refId": "A",
          "legendFormat": "Input",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "claude_code:token_total_by_type{type=\"output\"}",
          "refId": "B",
          "legendFormat": "Output",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "claude_code:token_total_by_type{type=\"cacheRead\"}",
          "refId": "C",
          "legendFormat": "Cache Read",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 48},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "auto",
        "colorMode": "value"
      }
    },
    {
      "id": 18,
      "title": "üíé Efficiency Metrics (All Time)",
      "type": "stat",
      "targets": [
        {
          "expr": "claude_code:lines_total_by_type{type=\"added\"} / claude_code:cost_total",
          "refId": "A",
          "legendFormat": "Lines per Dollar",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "claude_code:token_total_by_type{type=\"cacheRead\"} / (claude_code:token_total_by_type{type=\"input\"} + claude_code:token_total_by_type{type=\"cacheRead\"}) * 100",
          "refId": "B",
          "legendFormat": "Overall Cache Hit %",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 48},
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Lines per Dollar"},
            "properties": [{"id": "unit", "value": "short"}]
          },
          {
            "matcher": {"id": "byName", "options": "Overall Cache Hit %"},
            "properties": [{"id": "unit", "value": "percent"}]
          }
        ]
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "auto",
        "colorMode": "value"
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s",
  "version": 1,
  "schemaVersion": 30
}